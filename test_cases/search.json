      "endpoint": "search",
      "status": "pass",
      "in": {
        "size": 25,
        "text": "sons"
      },
      "expected": {
        "priorityThresh": 25,
        "properties": [
          {
            "id": "KVE:TopographicPlace:0301-Sons gate"
          }
        ]
      }
    }

  ]
