{
  "name": "Fuzzy search",
  "description": "Verify fuzzy search with common spelling mistakes / variations",
  "tests": [
    {
      "id": "oslo1",
      "endpoint": "autocomplete",
      "status": "pass",
      "in": {
        "text": "olso"
      },
      "expected": {
        "priorityThresh": 4,
        "properties": [
          {
            "id": "NSR:GroupOfStopPlaces:1"
          }
        ]
      }
    },
    {
      "id": "Oslo1-uppercase",
      "endpoint": "autocomplete",
      "status": "pass",
      "in": {
        "text": "Olso"
      },
      "expected": {
        "priorityThresh": 4,
        "properties": [
          {
            "id": "NSR:GroupOfStopPlaces:1"
          }
        ]
      }
    },
    {
      "id": "movig",
      "endpoint": "autocomplete",
      "status": "pass",
      "in": {
        "text": "møvig"
      },
      "expected": {
        "priorityThresh": 10,
        "properties": [
          {
            "name": "Møvik"
          }
        ]
      }
    },
    {
      "id": "wiens",
      "endpoint": "autocomplete",
      "status": "pass",
      "in": {
        "text": "wiens"
      },
      "expected": {
        "priorityThresh": 3,
        "properties": [
          {
            "id": "NSR:StopPlace:1043"
          }
        ]
      }
    }
  ]
}
