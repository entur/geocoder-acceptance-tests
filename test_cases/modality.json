          }
        ]
      }
    },
    {
      "id": "mannheller",
      "endpoint": "autocomplete",
      "status": "pass",
      "in": {
        "text": "mannheller ferjekai"
      },
      "expected": {
        "priorityThresh": 3,
        "properties": [
          {
