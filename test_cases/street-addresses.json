{
  "name": "Street addresses",
  "description": "Verify presence of street addresses",
  "tests": [
    {
      "id": "karjohansgate",
      "endpoint": "autocomplete",
      "status": "pass",
      "in": {
        "text": "karl johans gate 2"
      },
      "expected": {
        "priorityThresh": 1,
        "properties": [
          {
            "category": ["vegadresse"],
            "street": "Karl Johans gate",
            "name": "Karl Johans gate 2",
            "housenumber": "2",
            "label": "Karl Johans gate 2, Sarpsborg"
          }
        ]
      }
    },
    {
      "id": "karjohansgate-nonumber",
      "endpoint": "autocomplete",
